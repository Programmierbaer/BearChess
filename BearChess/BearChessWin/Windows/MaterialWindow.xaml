<Window x:Class="www.SoLaNoSoft.com.BearChessWin.MaterialWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:p="clr-namespace:www.SoLaNoSoft.com.BearChessWin.Properties"
        xmlns:local="clr-namespace:www.SoLaNoSoft.com.BearChessWin.Windows"
        xmlns:bearChessWin="clr-namespace:www.SoLaNoSoft.com.BearChessWin"
        ResizeMode="NoResize"
        mc:Ignorable="d" WindowStyle="ToolWindow" Closing="MaterialWindow_OnClosing"
        Title="{x:Static p:Resources.CapturedPieces}" Height="120" MinHeight="100" Width="520">
    <StackPanel>
        
        <bearChessWin:MaterialUserControl HorizontalAlignment="Left" VerticalAlignment="Top" x:Name="MaterialUserControl"></bearChessWin:MaterialUserControl>

    </StackPanel>
</Window>