    <Grid Name="gridBoard">
        <Grid.Resources>
            <Style x:Key="backgroundField" TargetType="Image">
                <Setter Property="Width" Value="{Binding ChessBackgroundFieldSize, FallbackValue='50'}" />
                <Setter Property="Height" Value="{Binding ChessBackgroundFieldSize, FallbackValue='50'}" />
            </Style>
            <Style TargetType="Image">
                <Setter Property="Width" Value="{Binding ChessFieldSize, FallbackValue='48'}" />
                <Setter Property="Height" Value="{Binding ChessFieldSize, FallbackValue='48'}" />
            </Style>
            <Style TargetType="Button">
                <Setter Property="Width" Value="{Binding ControlButtonSize}" />
                <Setter Property="Height" Value="{Binding ControlButtonSize}" />
            </Style>

            <Style  x:Key="controlButtons"  TargetType="Image">
                <Setter Property="Width" Value="{Binding ControlButtonSize, FallbackValue='40'}" />
                <Setter Property="Height" Value="{Binding ControlButtonSize, FallbackValue='40'}" />
            </Style>

